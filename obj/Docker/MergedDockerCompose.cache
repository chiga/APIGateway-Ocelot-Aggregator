services:
  api-gateway:
    build:
      context: C:\dev\APIGateway
      dockerfile: api-gateway/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: apigateway
    ports:
    - target: 80
    - target: 443
    volumes:
    - C:\Users\chiga\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\chiga\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
version: '3.4'